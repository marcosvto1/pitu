lassName=\"mb-3\">\n              <FormControl\n                placeholder=\"Informe sua URL para encurtar\"\n                defaultValue=\"\"\n                onChange={(e) => this.setState({ url: e.target.value })}\n              ></FormControl>\n              <InputGroup.Append>\n                <Button variant=\"primary\" type=\"submit\">\n                  Encurtar\n                </Button>\n              </InputGroup.Append>\n            </InputGroup>\n\n            {isLoading ? (\n              <Spinner animation=\"border\" />\n            ) : (\n              code && (\n                <>\n                  <InputGroup>\n                    <FormControl\n                      autoFocus={true}\n                      defaultValue={`${vars.API_HOST}/${code}`}\n                      ref={(input) => (this.inputURL = input)}\n                    ></FormControl>\n                    <InputGroup.Append>\n                      <Button\n                        variant=\"outline-secondary\"\n                        onClick={() => this.copyToClipBoart()}\n                      >\n                        Copiar\n                      </Button>\n                    </InputGroup.Append>\n                  </InputGroup>\n                  <p>\n                  Para acompanhar as estat√≠sticas acesse {vars.API_HOST}/{code}/stats\n                  </p>\n                </>\n              )\n            )}\n\n            {errorMessage && <Alert variant=\"danger\">{errorMessage}</Alert>}\n          </Form>\n        </ContentContainer>\n\n        <ContentContainer>\n          <AdsBlock>Adense</AdsBlock>\n        </ContentContainer>\n\n      </Container>\n    );\n  }\n}\n\nexport default HomePage;\n","/home/marcos/workspace/luiztools/pitu/frontend/src/pages/StatsPage/index.js",["69"],"/home/marcos/workspace/luiztools/pitu/frontend/src/routes/index.js",[],"/home/marcos/workspace/luiztools/pitu/frontend/src/pages/NotFoundPage/index.js",[],"/home/marcos/workspace/luiztools/pitu