    <Button variant=\"primary\" type=\"submit\">\n                  Encurtar\n                </Button>\n              </InputGroup.Append>\n            </InputGroup>\n\n            {isLoading ? (\n              <Spinner animation=\"border\" />\n            ) : (\n              code && (\n                <>\n                  <InputGroup>\n                    <FormControl\n                      autoFocus={true}\n                      defaultValue={`http://localhost:3000/${code}`}\n                      ref={(input) => (this.inputURL = input)}\n                    ></FormControl>\n                    <InputGroup.Append>\n                      <Button\n                        variant=\"outline-secondary\"\n                        onClick={() => this.copyToClipBoart()}\n                      >\n                        Copiar\n                      </Button>\n                    </InputGroup.Append>\n                  </InputGroup>\n                  <p>\n                    Para acompanhar as estat√≠sticas acesse http://localhost:3000/{code}/stats\n                  </p>\n                </>\n              )\n            )}\n\n            {errorMessage && <Alert variant=\"danger\">{errorMessage}</Alert>}\n          </Form>\n        </ContentContainer>\n\n        <ContentContainer>\n          <AdsBlock>Adense</AdsBlock>\n        </ContentContainer>\n\n      </Container>\n    );\n  }\n}\n\nexport default HomePage;\n","/home/marcos/workspace/luiztools/pitu/frontend/src/pages/StatsPage/index.js",[],["65","66"],"/home/marcos/workspace/luiztools/pitu/frontend/src/routes/index.js",[],"/home/marcos/workspace/luiztools/pitu/frontend/src/pages/NotFoundPage/index.js",[],"/home/marcos/workspace/luiztools/pitu/frontend/src/components/Header/index.js",[],["67","68"],"/home/marcos/workspace/luiztools/pitu/frontend/src/components/Header/styles.js",[],"/home/marcos/workspace/luiztools/pitu/frontend/src/services/api.js",[],"/home/marcos/workspace/luiztools/pitu/frontend/src/services/sho